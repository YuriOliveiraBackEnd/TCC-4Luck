@model IEnumerable<bibliotecaModel.ModelCliente>


@{
    if (Session["Funclogado"] != null)
    {
        Layout = "~/Views/Shared/_LayoutFunc.cshtml";
    }


}

<h2>Listar usuario</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.id_cli)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nome_cli)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CPF_cli)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tel_cli)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id_cli)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.nome_cli)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CPF_cli)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tel_cli)
            </td>
            <td>
                @Html.ActionLink("Detalhes", "DetalhesdoFunc", new { id = item.id_cli })
            </td>

        </tr>
    }

</table>
